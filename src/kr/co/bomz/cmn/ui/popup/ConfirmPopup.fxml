<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.FlowPane ?>
<?import javafx.scene.layout.VBox ?>
<?import javafx.scene.control.Label ?>
<?import javafx.scene.control.Button ?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="kr.co.bomz.cmn.ui.popup.ConfirmPopupController"
	stylesheets="kr/co/bomz/cmn/ui/popup/Popup.css">
	
	<FlowPane>
		<Label fx:id="contentLb" />
	</FlowPane>
	
	<FlowPane>
		<Button text="예" onAction="#handleOkAction" focusTraversable="false" />
		<Button text="아니요" onAction="#handleCancelAction" focusTraversable="false" />
	</FlowPane>
	
</VBox>

